{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "magdatafactoryadfutils"
		},
		"storagegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedatafactorydev01.dfs.core.windows.net"
		},
		"OnceAWeekTrigger_weekDays": {
			"type": "array",
			"defaultValue": [
				"Sunday"
			]
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://datafactorydevkv.vault.azure.net/"
		},
		"dataFactory_p1": {
			"type": "string",
			"defaultValue": "v2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "magdatafactoryadfutils_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dataFactory_p1": {
						"value": "[parameters('dataFactory_p1')]"
					},
					"storagegen2_properties_typeProperties_url": {
						"value": "[parameters('storagegen2_properties_typeProperties_url')]"
					},
					"OnceAWeekTrigger_weekDays": {
						"value": "[parameters('OnceAWeekTrigger_weekDays')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}