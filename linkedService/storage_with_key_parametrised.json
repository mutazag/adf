{
	"name": "storage_with_key_parametrised",
	"properties": {
		"parameters": {
			"secretname": {
				"type": "string",
				"defaultValue": "storageket"
			},
			"s1": {
				"type": "string",
				"defaultValue": "storage"
			},
			"s2": {
				"type": "string",
				"defaultValue": "ket"
			}
		},
		"annotations": [],
		"type": "AzureBlobFS",
		"typeProperties": {
			"url": "https://magdatafactorystordev.dfs.core.windows.net/",
			"accountKey": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@{concat(linkedService().s1,replace(linkedService().s2,'ket','key'))}",
					"type": "Expression"
				}
			}
		}
	}
}